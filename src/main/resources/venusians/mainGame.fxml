<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<AnchorPane
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="venusians.MainGameController"
>
	<children>
		<VBox
			alignment="CENTER"
			spacing="20.0"
		>
			<children>
				<Label
					text="Main Game"
				 />
				<Button
					fx:id="secondaryButton"
					text="Switch to Results"
					onAction="#gameEnded"
				 />
			</children>
			<padding>
				<Insets
					bottom="20.0"
					left="20.0"
					right="20.0"
					top="20.0"
				 />
			</padding>
		</VBox>
	</children>
</AnchorPane>