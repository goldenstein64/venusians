<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="50.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="venusians.gui.main.artists.developmentToolTip.DevelopmentToolTipController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" height="${rootPane.height}" layoutY="-60.0" opacity="0.5" stroke="BLACK" strokeType="INSIDE" width="${rootPane.width}" />
      <VBox alignment="CENTER" layoutY="-60.0" prefHeight="50.0" prefWidth="200.0">
         <children>
            <Label fx:id="nameLabel" text="Knight" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label fx:id="descriptionLabel" text="Move the Robber." textFill="WHITE" />
         </children>
      </VBox>
   </children>
</AnchorPane>